{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "prettier",
    "prettier/standard",
    "plugin:prettier/recommended",
    "eslint:recommended"
  ],
  "rules": {
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "backtick"],
    "semi": [2, "never"],
    "prettier/prettier": "error",
    "no-unexpected-multiline": "error",
    "no-useless-concat": "error",
    "prefer-template": "error",
    "no-prototype-builtins": "off",
    "no-extra-semi": "off"
  }
}
